{
    "version": "0.1.0",
    "configurations": [
      {
          "type": "chrome",
          "request": "attach",
          "name": "Attach Chrome",
          "port": 9222,
          "url": "http://localhost:3000*",
          "webRoot": "${workspaceRoot}/src",
          "sourceMapPathOverrides": {
              "webpack:///src/client/*": "${webRoot}/*"
          }
      }
    ]
}
